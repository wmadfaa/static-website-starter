doctype html
html(lang='en')
  head

    //- Meta
    meta(charset='UTF-8')
    meta(name='viewport', content='width=device-width, initial-scale=1, maximum-scale=1')
    meta(http-equiv='X-UA-Compatible', content='ie=edge')

    meta(name='theme-color', content='#000000')

    //- SEO

    //- Basic Seo
    block basicSeo
      meta(name='description' content='Index page')
      meta(name='keywords', content='index, page')
      meta(name='author' content='')

    //- Twitter
    meta(name='twitter:card', content='summary')
    meta(name='twitter:site', content='')
    meta(name='twitter:image', content='')
    meta(name='twitter:description', content='')

    //- Facebook
    meta(property='og:url', content='')
    meta(property='og:title', content='')
    meta(property='og:description', content='')
    meta(property='og:image', content='')
    meta(property='og:type', content='website')

    //- Favicons
    link(rel='apple-touch-icon', sizes='192x192', href='public/logo192.png')
    link(rel='shortcut icon', href='public/favicon.ico')

    //- Preload fonts
    link(type="font/ttf", href=require('src/assets/fonts/Roboto/Roboto-Black.ttf'), rel="preload", as="font" crossorigin)
    link(type="font/ttf", href=require('src/assets/fonts/Roboto/Roboto-BlackItalic.ttf'), rel="preload", as="font" crossorigin)
    link(type="font/ttf", href=require('src/assets/fonts/Roboto/Roboto-Bold.ttf'), rel="preload", as="font" crossorigin)
    link(type="font/ttf", href=require('src/assets/fonts/Roboto/Roboto-BoldItalic.ttf'), rel="preload", as="font" crossorigin)
    link(type="font/ttf", href=require('src/assets/fonts/Roboto/Roboto-Italic.ttf'), rel="preload", as="font" crossorigin)
    link(type="font/ttf", href=require('src/assets/fonts/Roboto/Roboto-Light.ttf'), rel="preload", as="font" crossorigin)
    link(type="font/ttf", href=require('src/assets/fonts/Roboto/Roboto-LightItalic.ttf'), rel="preload", as="font" crossorigin)
    link(type="font/ttf", href=require('src/assets/fonts/Roboto/Roboto-Medium.ttf'), rel="preload", as="font" crossorigin)
    link(type="font/ttf", href=require('src/assets/fonts/Roboto/Roboto-MediumItalic.ttf'), rel="preload", as="font" crossorigin)
    link(type="font/ttf", href=require('src/assets/fonts/Roboto/Roboto-Regular.ttf'), rel="preload", as="font" crossorigin)
    link(type="font/ttf", href=require('src/assets/fonts/Roboto/Roboto-Thin.ttf'), rel="preload", as="font" crossorigin)
    link(type="font/ttf", href=require('src/assets/fonts/Roboto/Roboto-ThinItalic.ttf'), rel="preload", as="font" crossorigin)

    //- Title
    block title
      title Your project!

    //- Manifest
    block manifestBrowserconfigFiles
      link(rel='manifest', href='public/manifest.json')

  body

    //- block content
    block content

    //- Footer
    include ../components/footer.pug

    //- block scripts
    block scripts
